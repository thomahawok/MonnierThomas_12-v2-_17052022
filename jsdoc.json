{
    "source":{
        "include" : ["src"],
        "includePattern": ".jsx$",
        "excludePattern" : "(nodes_modules/|docs)"
    },

    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "recuse":true,
        "destination":"./docs"
    }
}